<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.vanmulligen.bitcoinpricetracker">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ca.vanmulligen.bitcoinpricetracker.views.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ca.vanmulligen.bitcoinpricetracker.views.settings.SettingsActivity" android:parentActivityName="ca.vanmulligen.bitcoinpricetracker.views.main.MainActivity" android:label="@string/settings_activity_title">            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="ca.vanmulligen.bitcoinpricetracker.views.main.MainActivity" />
        </activity>
    </application>

</manifest>